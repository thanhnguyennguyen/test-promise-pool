{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "prettier"
  ],
  "plugins": [
    "prettier",
    "json-format"
  ],
  "rules": {
    "prettier/prettier": "error",
    "semi": 0
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}
